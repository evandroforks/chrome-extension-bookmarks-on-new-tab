{
  "name": "Ultra light new tab",
  "version": "0.1.0.0",

  "description": "Ultra light new tab with bookmark navigator",

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },

  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "permissions": [
    "storage",
    "activeTab",
    "chrome://favicon/"
  ],

  "optional_permissions": [
    "bookmarks"
  ],

  "homepage_url": "http://michurin.github.io/",

  "manifest_version": 2
}
